{
  "name": "ai-chatbot",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "./scripts/pull-qwen.sh",
    "dev": "concurrently \"node --watch src/server.js\" \"./scripts/livereload.sh\"",
