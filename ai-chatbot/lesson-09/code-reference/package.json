    "model:pull": "./scripts/pull-qwen.sh"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "livereload": "^0.10.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "marked": "^16.4.0"
  }
}
